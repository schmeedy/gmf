# Debugging options for the org.eclipse.gmf.runtime.common.ui.pathvariables.ide plug-in

# Turn on general debugging for the com.ibm.xtools.modeler plug-in
org.eclipse.gmf.runtime.common.ui.pathvariables.ide/debug=true

# Trace when exceptions are caught
org.eclipse.gmf.runtime.common.ui.pathvariables.ide/debug/exceptions/catching=false
# Trace when exceptions are thrown
org.eclipse.gmf.runtime.common.ui.pathvariables.ide/debug/exceptions/throwing=false

# Trace when methods are entered
org.eclipse.gmf.runtime.common.ui.pathvariables.ide/debug/methods/entering=false

# Trace when methods are exited
org.eclipse.gmf.runtime.common.ui.pathvariables.ide/debug/methods/exiting=false

